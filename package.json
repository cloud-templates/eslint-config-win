{
  "name": "@winner-fed/eslint-config-win",
  "version": "2.1.0",
  "description": "winner-fed 前端编码规范文档",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "cloudwin",
    "javascript",
    "styleguide",
    "vue",
    "vue3"
  ],
  "homepage": "https://github.com/cloud-templates/eslint-config-win#readme",
  "bugs": {
    "url": "https://github.com/cloud-templates/eslint-config-win/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloud-templates/eslint-config-win.git"
  },
  "license": "MIT",
  "author": "winner-fed",
  "main": "index.js",
  "files": [
    "index.js",
    "base.js",
    "vue.js",
    "typescript.js",
    "vue3.js",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "pnpm run dev",
    "dev": "run-p dev:*",
    "dev:eslintrc": "nodemon",
    "dev:copyfiles": "pnpm run build:copyfiles && chokidar \"site/public/**/*\" -c \"pnpm run build:copyfiles\"",
    "dev:webpack-dev-server": "webpack serve --open",
    "build": "run-s build:*",
    "build:eslintrc": "ts-node scripts/build.ts",
    "build:clean": "rimraf ./dist",
    "build:copyfiles": "copyfiles -u 2 \"site/public/**/*\" dist/public",
    "build:site": "webpack",
    "clean": "rimraf dist node_modules && pnpm -r exec rm -rf node_modules dist",
    "test": "pnpm run lint && ts-node ./test/index.ts",
    "lint": "run-s eslint prettier",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-pattern \"bad.*\" .",
    "rulesCoverage": "ts-node ./scripts/rulesCoverage.ts",
    "prettier": "prettier -l \"./**/*\"",
    "prettier:fix": "prettier --write -l \"./**/*\"",
    "prepare": "husky install",
    "update": "pnpm up --latest",
    "release:dry": "node scripts/release.js --dry",
    "release": "node scripts/release.js"
  },
  "lint-staged": {
    "package.json": "sort-package-json"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@types/cookie": "^0.5.1",
    "@types/doctrine": "^0.0.5",
    "@types/eslint": "^8.44.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.4.5",
    "@types/prettier": "^2.7.3",
    "@types/prompts": "^2.0.14",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@types/rimraf": "^4.0.5",
    "@types/semver": "^7.3.12",
    "@types/xml-escape": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "chalk": "^4.1.2",
    "chokidar-cli": "^3.0.0",
    "cookie": "^0.5.0",
    "copyfiles": "^2.4.1",
    "doctrine": "^3.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-vue": "^9.15.1",
    "execa": "5.1.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "insert-tag": "^0.1.2",
    "lint-staged": "^13.2.3",
    "minimist": "^1.2.6",
    "mobi-plugin-color": "^1.0.0",
    "mobi.css": "^3.1.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "prompts": "^2.4.2",
    "querystring": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "4.5.1",
    "rimraf": "^5.0.1",
    "semver": "^7.3.7",
    "sort-package-json": "^2.5.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "url": "^0.11.1",
    "vue-eslint-parser": "^9.3.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "xml-escape": "^1.1.0"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "7.x",
    "@babel/preset-react": "7.x",
    "@typescript-eslint/eslint-plugin": ">=5.55.0",
    "@typescript-eslint/parser": ">=5.0.0",
    "eslint": ">=8.24.0",
    "eslint-plugin-react": ">=7.31.8",
    "eslint-plugin-vue": ">=9.5.1",
    "typescript": "5.x",
    "vue-eslint-parser": "9.x"
  },
  "peerDependenciesMeta": {
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/preset-react": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">= 14.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
